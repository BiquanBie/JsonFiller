{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "rewrites": [
    {
      "source": "/dev",
      "destination": "/"
    },
    {
      "source": "/dev/(.*)",
      "destination": "/$1"
    }
  ]
}